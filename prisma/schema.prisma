generator client {
  provider = "prisma-client-js"
  output   = "./zod"
  imports  = "../src/zod-schemas"
}

datasource db {
  provider  = "postgresql"
  url  	    = env("DATABASE_URL")
}

model Job {
  id String @id @default(uuid()) 
  company String
  position String
  location String?
  country String?
  salary Int
  isAvailable Boolean @default(true)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
